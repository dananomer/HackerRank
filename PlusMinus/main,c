void plusMinus(int arr_count, int* arr) {
    float count_pos = 0;
    float count_neg = 0;
    float count_zero = 0;
    
    for(int i=0;i<arr_count;i++){
        if(arr[i] > 0) count_pos++;
        else if(arr[i] < 0) count_neg++;
        else count_zero++;
    }
    printf("%f\n",(float)count_pos/arr_count);
    printf("%f\n",(float)count_neg/arr_count);
    printf("%f\n",(float)count_zero/arr_count);
}
